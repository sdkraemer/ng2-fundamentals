{
  "name": "ng2-fundamentals-demo",
  "version": "1.0.0",
  "engines": {
    "node": "5.9.1"
  },
 "main": "node_modules/ng2f-server/server.js",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run server\" ",
    "lite": "lite-server",
    "server": "node node_modules/ng2f-server/server.js",
    "dev": "nodemon node_modules/ng2f-server/server.js -w ./node_modules/ng2f-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "bundle": "node node_modules/ng2f-server/bundler.js",
    "bundle:prod": "node node_modules/ng2f-server/bundler.js --prod"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.1",
    "@angular/compiler": "2.0.0-rc.1",
    "@angular/core": "2.0.0-rc.1",
    "@angular/http": "2.0.0-rc.1",
    "@angular/platform-browser": "2.0.0-rc.1",
    "@angular/platform-browser-dynamic": "2.0.0-rc.1",
    "@angular/router": "2.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.1",
    "@angular/upgrade": "2.0.0-rc.1",
    "core-js": "^2.4.0",
    "jquery": "^2.2.4",
    "ng2f-bootstrap": "0.0.3",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.27",
    "toastr": "^2.1.2",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "systemjs-builder": "^0.15.19",
    "typescript": "^1.8.10",
    "typings": "^1.0.5",
    "yargs": "^4.7.1"
  }
}
